{
  "listeners": {
    "*:80": {
      "pass": "routes"
    }
  },
  "routes": [
    {
      "action": {
        "share": "/app/public$uri",
        "fallback": {
          "pass": "applications/symfony/index"
        }
      }
    }
  ],
  "applications": {
    "symfony": {
      "type": "php",
      "processes": 20,
      "user": "www-data",
      "group": "www-data",
      "targets": {
        "index": {
          "root": "/app/public/",
          "script": "index.php"
        }
      }
    }
  }
}
